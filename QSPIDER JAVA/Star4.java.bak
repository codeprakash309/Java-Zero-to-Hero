import java.util.Scanner;
class Star4 
{
	public static void main(String[] args) 
	{
		Scanner scn= new Scanner(System.in);
		System.out.println("Enter number of row : ");
		int row=scn.nextInt();
		System.out.println("Enter number of column : ");
		int col=scn.nextInt();
		System.out.println();
		for(int i=1;i<=row;i++){
             for(int j=col;j>=1;j--){
				System.out.print("*");
		}
		System.out.println();
	}
}
}