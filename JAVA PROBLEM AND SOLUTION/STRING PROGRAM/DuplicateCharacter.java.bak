//write  a program to find Duplicate Character in a given string
import java.util.Scanner;
class  DuplicateCharacter
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter a String  : ");
		String str=sc.next();
		String ch="";
		String duplicate="";
		for(int i=0;i<str.length();i++)
		{
          String current=Character.toString(str.charAt(i));
			if(ch.contains(current))
			{
				if(!duplicate.contains(current))    //the contains() method checks whether a string contains a sequence of characters.
			    {                                                           //returns true if the character exist and false if not
				     duplicate+=current+",";
		     	}
		     }
		   ch+=current;
		}
		System.out.println("duplicate character :  "+duplicate);
	}
}
