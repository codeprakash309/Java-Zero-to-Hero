import java.util.Scanner;
class ReverseSubArrayExample2 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the array size :");
		int n=sc.nextInt();
		int arr[]=new int[n];
		System.out.print("Enter the Array element into given array size of an array : ");
		for(int i=0;i<n;i++)
		{
        arr[i]=sc.nextInt();
		}
		System.out.print("Enter the size of sub array : ");
		int subSize=sc.nextInt();
		for(int i=0;i<arr.length;i+=subSize)
		{
          int left=i;
		  int right=Math.min(i+subSize-1,n-1);
		  int temp;
		  while(left<right)
			{
               temp=arr[left];
			   arr[left]=arr[right];
			   arr[right]=temp;
			   left++;
			   right--;

			} //ends while loop
		} //ends for loop
		System.out.println("the reverse of sub-array of an given array elements are : ");
		for(int i=0;i<arr.length;i++)
		{
          System.out.println(arr[i]);
		}
	}
}
