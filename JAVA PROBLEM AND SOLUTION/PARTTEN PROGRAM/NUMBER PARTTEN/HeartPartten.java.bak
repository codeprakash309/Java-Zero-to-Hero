import java.util.Scanner;
class HeartPartten 
{
	public static void main(String[] args) 
	{
		//Scanner sc= new Scanner(System.in);
		//System.out.print("Enter the number of row and column : ");
		//int n=sc.nextInt();
		int n=9;
		for(int i=0;i<n;i++)
		{
        for(int j=0;j<n;j++)
			{
             if((i==0&&j%3!=0)||(i==1&&j%3==0)||(i-j==2||i+j==8))
				{
                  System.out.print("*");
				}
				else
				{
               System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
