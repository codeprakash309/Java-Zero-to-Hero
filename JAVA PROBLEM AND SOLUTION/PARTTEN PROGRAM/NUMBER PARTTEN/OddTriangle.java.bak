import java.util.Scanner;
class OddTriangle 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Entre the number of row and column : ");
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
		{ 
			for(int j=0;j<n;j++)
			{
				if(i>=j)
		       System.out.print(i+"");
			}
             System.out.println();
		}
	}
}
