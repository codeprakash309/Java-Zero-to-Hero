import java.util.Scanner;
class NumberRightTriangle 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Entre the number of row and column : ");
		int n=sc.nextInt();
		for(int i=1;i<=n;i++)
		{ 
			for(int j=1;j<=n;j++)
			{
				if((i+j<=n&&i<=j)||(i+j>=n&&i>=j))
		       System.out.print("");
				else
					System.out.print(i);
			}
             System.out.println();
		}
	}
}
