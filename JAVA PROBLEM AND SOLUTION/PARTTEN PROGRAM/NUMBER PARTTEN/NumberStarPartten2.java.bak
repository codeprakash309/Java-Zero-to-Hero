import java.util.Scanner;
class NumberStarPartten2 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the number of row  : ");
		int n=sc.nextInt();
		for(int i=1;i<=n;i++)
		   {
          for(int j=i;j<=2*i;j++)
			   {
                   System.out.print(j);
			   }
			   System.out.println();
	    	}
		
	}
}
