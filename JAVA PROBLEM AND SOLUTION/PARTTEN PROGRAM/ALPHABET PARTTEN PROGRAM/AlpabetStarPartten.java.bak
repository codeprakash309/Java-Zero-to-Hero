import java.util.Scanner;
class AlpabetStarPartten 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the number of row  : ");
		int n=sc.nextInt();
		for(int i=n;i>=1;i--)
		   {
          for(int k=n-1;k>=i;k--)
			{
			  System.out.print(" ");

			}
			for(int j=i;j>=1;j--)
			   {
              System.out.println(i);
			   }
			   System.out.println();
		}
		
	}
}
